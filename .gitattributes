# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
*.yaml filter=lfs diff=lfs merge=lfs -text
*.gitignore filter=lfs diff=lfs merge=lfs -text
*.tres filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.import filter=lfs diff=lfs merge=lfs -text
*.pdf filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text
*.txt filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.svg filter=lfs diff=lfs merge=lfs -text
*.aseprite filter=lfs diff=lfs merge=lfs -text
*.webp filter=lfs diff=lfs merge=lfs -text
*.gdshader filter=lfs diff=lfs merge=lfs -text
*.ttf filter=lfs diff=lfs merge=lfs -text
*.tscn filter=lfs diff=lfs merge=lfs -text
*.gd filter=lfs diff=lfs merge=lfs -text
*.cfg filter=lfs diff=lfs merge=lfs -text
*.pck filter=lfs diff=lfs merge=lfs -text
*.x86_64 filter=lfs diff=lfs merge=lfs -text
*.js filter=lfs diff=lfs merge=lfs -text
*.html filter=lfs diff=lfs merge=lfs -text
*.wasm filter=lfs diff=lfs merge=lfs -text
*.exe filter=lfs diff=lfs merge=lfs -text
*.json filter=lfs diff=lfs merge=lfs -text
*.godot filter=lfs diff=lfs merge=lfs -text
* !text !filter !merge !diff
/export/linux/Lightkeepers.x86_64 filter=lfs diff=lfs merge=lfs -text
/export/web/lightkeepers.side.wasm filter=lfs diff=lfs merge=lfs -text
/export/windows/Lightkeepers.exe filter=lfs diff=lfs merge=lfs -text
/exports/linux/Lightkeepers.x86_64 filter=lfs diff=lfs merge=lfs -text
/exports/web/lightkeepers.side.wasm filter=lfs diff=lfs merge=lfs -text
/exports/windows/Lightkeepers.exe filter=lfs diff=lfs merge=lfs -text
